<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ProfileMenuRow">
        <attr name="iconImage" format="string" />
        <attr name="name" format="string" />
        <attr name="description" format="string" />
    </declare-styleable>
    <declare-styleable name="PerilCategoryView">
        <attr name="categoryUrl" format="string" />
        <attr name="title" format="string" />
        <attr name="subtitle" format="string" />
    </declare-styleable>
    <declare-styleable name="PerilView">
        <attr name="perilIcon" format="string" />
        <attr name="perilText" format="string" />
    </declare-styleable>

    <attr name="themeBackgroundColor" format="reference" />
    <attr name="themeBackgroundColorHighlight" format="reference" />
    <attr name="themeTextColor" format="reference" />
    <attr name="themeLightWindowBar" format="boolean" />
</resources>
